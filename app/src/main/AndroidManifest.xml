<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".MoneyManagerAPP"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoneyManager"
        tools:targetApi="31">
        <activity
            android:name=".UI.settings.SettingsActivity"
            android:exported="false" />
        <activity
            android:name=".UI.archivos.ArchivoActivity"
            android:exported="false" />
        <activity
            android:name=".UI.liquidaciones.LiquidacionesActivity"
            android:exported="false" />
        <activity
            android:name=".UI.gastos.view.GastosActivity"
            android:exported="false" />
        <activity
            android:name=".UI.dashboard.Inicio"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UI.Usuarios.view.HomeActivity"
            android:exported="false" />
    </application>

</manifest>